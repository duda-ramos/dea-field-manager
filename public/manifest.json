{
  "manifest_version": 3,
  "name": "DEA Manager Extension",
  "version": "1.0.0",
  "description": "Chrome extension for DEA Manager integration",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}